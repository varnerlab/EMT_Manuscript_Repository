	Mesh = {Animals; Cell Division; Cell Nucleus; Cell Transformation, Neoplastic; Down-Regulation; Fibronectins; Genes, ras; Glycosylation; Intestinal Mucosa; Ligands; Protein Binding; Proto-Oncogene Proteins p21(ras); RNA, Neoplasm; Rats; Receptors, Transforming Growth Factor beta; Transforming Growth Factor beta; Tumor Cells, Cultured},
	Month = {Dec},
