	Mesh = {Cell Cycle Checkpoints; Cell Death; Computer Simulation; DNA Damage; Feedback, Physiological; Humans; Imidazoles; MCF-7 Cells; Models, Biological; Phosphoprotein Phosphatases; Piperazines; Proto-Oncogene Proteins c-mdm2; RNA Interference; Signal Transduction; Time-Lapse Imaging; Tumor Suppressor Protein p53},
	Month = {Nov},
