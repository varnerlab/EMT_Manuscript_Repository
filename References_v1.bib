	Doi = {10.1073/pnas.111614398},
	Institution = {Departments of Medicine and Molecular Genetics, Albert Einstein College of Medicine, Bronx, NY 10461, USA.},
	Journal = {Proc Natl Acad Sci U S A},
	Keywords = {Cell Adhesion, drug effects; Cell Movement, drug effects; Cells, Cultured; Cytoskeleton, physiology; Epithelial Cells, physiology; Gene Expression Regulation, drug effects; Humans; Integrins, physiology; Keratinocytes, physiology; Mitogen-Activated Protein Kinases, physiology; Transcription, Genetic, drug effects; Transforming Growth Factor beta, pharmacology},
