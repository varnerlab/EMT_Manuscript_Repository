	Date-Modified = {2011-04-23 17:23:43 -0400},
	Doi = {10.1083/jcb.200601018},
	Journal = {J Cell Biol},
	Journal-Full = {The Journal of cell biology},
	Mesh = {Animals; Cell Differentiation; Epithelial Cells; Epithelium; Female; Humans; Mesoderm; Neoplasm Invasiveness; Neoplasms; Signal Transduction},
	Month = {Mar},
	Number = {7},
	Pages = {973-81},
	Pmc = {PMC2063755},
	Pmid = {16567498},
	Pst = {ppublish},
	Title = {The epithelial-mesenchymal transition: new insights in signaling, development, and disease},
	Volume = {172},
