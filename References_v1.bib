	Journal = {Cancer Res},
	Journal-Full = {Cancer research},
	Mesh = {Cell Line, Tumor; Cell Movement; Gene Expression Regulation, Neoplastic; Humans; Mesoderm; Pancreatic Neoplasms; Promoter Regions, Genetic; RNA, Small Interfering; Sp1 Transcription Factor; Transfection; Transforming Growth Factor beta; Vimentin},
