	Keywords = {dynamic modelling; identification; inference; reverse engineering; systems biology},
	Mesh = {Artificial Intelligence; Automation; Bayes Theorem; Bioengineering; Biophysics; Markov Chains; Models, Biological; Models, Statistical; Monte Carlo Method; Systems Biology},
