	Journal = {Cell Signal},
	Keywords = {Animals; Humans; Mice; Neoplasm Invasiveness; Neoplasms, drug therapy/etiology/metabolism; Proteoglycans, chemistry/metabolism/physiology; Receptors, Transforming Growth Factor beta, chemistry/metabolism/physiology; Signal Transduction},
